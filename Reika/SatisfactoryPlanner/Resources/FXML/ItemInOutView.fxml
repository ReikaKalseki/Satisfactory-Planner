<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="rootGrid" alignment="CENTER_LEFT" hgap="4.0" maxHeight="32.0" stylesheets="@../CSS/style.css" vgap="4.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Reika.SatisfactoryPlanner.GUI.Components.ItemInOutViewController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" />
      <ColumnConstraints hgrow="SOMETIMES" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <HBox fx:id="inputs" alignment="CENTER" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="12.0" GridPane.columnIndex="1" />
      <HBox fx:id="outputs" alignment="CENTER" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="12.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label fx:id="inputLabel" text="Input" />
      <Label fx:id="outputLabel" layoutX="10.0" layoutY="10.0" text="Output" GridPane.rowIndex="1" />
   </children>
   <padding>
      <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
   </padding>
   <styleClass>
      <String fx:value="widget" />
      <String fx:value="inner-panel" />
   </styleClass>
</GridPane>
