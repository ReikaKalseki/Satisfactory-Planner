<GridPane fx:id="rootGrid" alignment="CENTER_LEFT" hgap="4.0" maxHeight="32.0" style="-fx-border-width: 2; -fx-border-color: #aaa; -fx-border-radius: 3;" vgap="4.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Reika.SatisfactoryPlanner.GUI.ItemInOutViewController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" />
